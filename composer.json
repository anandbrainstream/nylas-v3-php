{
    "name": "brainstream/nylas-php",
    "description": "Nylas PHP SDK for api version 3",
    "keywords": [
        "nylas",
        "nylas-v3-api-php",
        "email",
        "contacts",
        "calendar",
        "php"
    ],
    "license": "MIT",
    "authors" : [
        {
          "name": "BrainStream Technolabs",
          "email": "support@brainstream.tech"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.7",
        "respect/validation": "^2.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "phpunit/phpunit": "^10.2",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^6.4",
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Nylas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --configuration tests/phpunit.xml --do-not-cache-result"
    }
}
